{
  "name": "typefeel",
  "description": "The client for typefeel.com",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev -p 5000",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-file-dropzone": "^0.0.12"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.9",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tailwindcss/ui": "^0.7.2",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.9",
    "@types/polka": "^0.5.1",
    "@urql/svelte": "^1.1.3",
    "body-parser": "^1.19.0",
    "cross-fetch": "^3.0.6",
    "cssnano": "^4.1.10",
    "dayjs": "^1.9.6",
    "faunadb": "^4.0.0",
    "graphql": "^15.4.0",
    "lodash": "^4.17.20",
    "marked": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.2.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "shortid": "^2.2.16",
    "svelte": "^3.17.3",
    "svelte-apollo": "^0.4.0",
    "svelte-check": "^1.0.46",
    "svelte-preprocess": "^4.3.0",
    "tailwindcss": "^1.9.6",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
